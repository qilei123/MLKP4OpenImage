train_net: "models/ResNet/train10w.prototxt"
base_lr: 0.000001
lr_policy: "step"
gamma: 0.1
stepsize: 5000
display: 20
average_loss: 1
# iter_size: 1
momentum: 0.9
weight_decay: 0.00005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "resnet_mlkp"
iter_size: 2
